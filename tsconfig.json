{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": true,
        "outDir": "target/"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts",
        "!./target/**/*.ts",
        "./typings/**/*.ts",
        "!./output/**/*.ts",
        "!./test_files/**/*.ts",
        "!./hand_written_extra/**/*.ts"
    ],
    "files": [
        "./DojoDetailsInterface.ts",
        "./generate_all.ts",
        "./generator.ts",
        "./main.ts",
        "./test.ts",
        "./typings/he/he.d.ts",
        "./typings/lib.es6.d.ts",
        "./typings/lodash/lodash.d.ts",
        "./typings/node/node.d.ts",
        "./typings/nodeunit/nodeunit.d.ts",
        "./typings/shelljs/shelljs.d.ts"
    ]
}
